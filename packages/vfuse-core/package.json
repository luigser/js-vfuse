{
  "name": "vfuse-core",
  "version": "1.0.0",
  "description": "vfuse-core",
  "main": "src/index.js",
  "browser": {
    "fs": false,
    "path": false,
    "os": false,
    "tls": false,
    "net": false,
    "timers": false,
    "stream": false,
    "zlib": false,
    "http": false,
    "crypto": false,
    "https": false,
    "tty": false,
    "dns": false,
    "child_process": false,
    "dgram": false,
    "process": false
  },
  "node": {
    "global": true,
    "__filename": false,
    "__dirname": false
  },
  "scripts": {
    "build": "aegir build",
    "docs": "aegir docs",
    "lint": "aegir lint",
    "test": "aegir test",
    "test:node": "aegir test -t node",
    "test:browser": "aegir test --target browser",
    "test:webworker": "aegir test --target webworker",
    "test:electron-main": "aegir test --target electron-main",
    "test:electron-renderer": "aegir test --target electron-renderer",
    "coverage": "nyc --reporter=text --reporter=lcov npm run test:node",
    "clean": "rimraf ./dist",
    "dep-check": "aegir dep-check -i interface-ipfs-core -i ipfs-core-types -i abort-controller",
    "prod": "webpack",
    "node-bundle": "webpack --config webpack.config.node.js",
    "web-bundle": "webpack --config webpack.config.web.js"
  },
  "author": "Luigi Serra",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "aegir": "^37.0.14",
    "assert": "^2.0.0",
    "babel-loader": "^8.2.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-polyfill": "^6.26.0",
    "browserify": "^17.0.0",
    "chai": "^4.3.4",
    "chai-http": "^4.3.0",
    "cluster": "^0.7.7",
    "debug": "^4.3.1",
    "dirty-chai": "^2.0.1",
    "file-loader": "^6.2.0",
    "repl": "^0.1.3",
    "terser-webpack-plugin": "^5.3.1",
    "url-loader": "^4.1.1",
    "v8": "^0.1.0",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "@nftstorage/ipfs-cluster": "^3.3.1",
    "browser-or-node": "^1.3.0",
    "fflate": "^0.7.3",
    "http-proxy": "^1.18.1",
    "ipfs": "^0.60.0",
    "ipfs-cluster-api": "0.0.9",
    "ipfs-http-client": "^52.0.5",
    "ipfs-http-server": "^0.5.2",
    "it-pipe": "^1.1.0",
    "libp2p-bootstrap": "^0.12.3",
    "libp2p-delegated-content-routing": "^0.11.0",
    "libp2p-delegated-peer-routing": "^0.10.0",
    "libp2p-floodsub": "^0.27.1",
    "libp2p-gossipsub": "^0.11.4",
    "libp2p-kad-dht": "^0.21.0",
    "libp2p-mdns": "^0.17.0",
    "libp2p-mplex": "^0.10.3",
    "libp2p-noise": "^3.0.0",
    "libp2p-secio": "^0.13.1",
    "libp2p-tcp": "^0.17.2",
    "libp2p-webrtc-direct": "^0.6.0",
    "libp2p-webrtc-star": "^0.23.0",
    "libp2p-websocket-star": "^0.10.2",
    "libp2p-websockets": "^0.15.7",
    "localdata": "^0.1.3",
    "lodash": "^4.17.21",
    "lzutf8": "^0.6.0",
    "mathjs": "^10.0.0",
    "multiformats": "^9.4.9",
    "nanoid": "^3.1.25",
    "node-fetch": "^2.6.6",
    "npm": "^6.14.4",
    "peer-id": "^0.14.8",
    "uint8arrays": "^2.1.5",
    "underscore": "^1.13.1",
    "wrtc": "^0.4.7",
    "ws": "^8.5.0"
  },
  "eslintConfig": {
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/explicit-function-return-type": "warn",
      "@typescript-eslint/strict-boolean-expressions": "off"
    },
    "ignorePatterns": [],
    "env": {
      "browser": true,
      "node": true
    }
  },
  "contributors": [
    "Luigi Setta <lserra@unisa.it>"
  ],
  "files": [
    "src/components",
    "dist"
  ],
  "build": {
    "files": [
      {
        "from": "node_modules/leveldown/prebuilds/${platform}-${arch}",
        "to": "prebuilds/${platform}-${arch}"
      }
    ]
  },
  "aegir": {
    "tsRepo": false
  }
}
